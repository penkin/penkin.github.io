---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>{{ site.title | xml_escape }}</title>
	<id>{{ site.url }}{{ site.baseurl }}/</id>
	<link rel="alternate" href="{{ site.url }}{{ site.baseurl }}/"/>
	<link rel="self" href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}"/>
	<updated>{{ site.time | date_to_rfc822 }}</updated>
	<author>
		<name>{{ site.social.name }}</name>
	</author>
  {%- for post in site.posts limit:10 -%}
	<entry>
		<title>{{ post.title | xml_escape }}</title>
		<link rel="alternate" type="text/html" href="{{ post.url | prepend: site.baseurl | prepend: site.url }}"/>
		<id>{{ post.url | prepend: site.baseurl | prepend: site.url }}</id>
		<published>{{ post.date | date_to_rfc822 }}</published>
		<updated>{{ post.date | date_to_rfc822 }}</updated>
		<content type="html">{{ post.content }}</content>
    {%- for tag in post.tags -%}
    <category>{{ tag | xml_escape }}</category>
    {%- endfor -%}
    {%- for cat in post.categories -%}
    <category>{{ cat | xml_escape }}</category>
    {%- endfor -%}
	</entry>
  {%- endfor -%}
</feed>
